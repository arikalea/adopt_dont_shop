<%= form_with url: path, method: method, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, value: name %><br>
  <%= f.label :address %>
  <%= f.text_field :address, value: address %><br>
  <%= f.label :city %>
  <%= f.text_field :city, value: city %><br>
  <%= f.label :state %>
  <%= f.text_field :state, value: state %><br>
  <%= f.label :zip %>
  <%= f.text_field :zip, value: state %><br>
  <%= f.submit button_text %>
<% end %>
